import java.util.Scanner;
public class prog {

   public static void main(String[] args)

{
   int i,a;
   
   System.out.print("Enter input a ");

   Scanner scan = new Scanner(System.in);
		    a = scan.nextInt();
        
        
  System.out.println ("\nThe  numbers are :");
   for(i=1;i<=a;i++)
